<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="255.0" prefWidth="461.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rpgteambuilder.SignUpController">
   <children>
      <Label layoutX="137.0" layoutY="66.0" text="Username:" />
      <Label layoutX="140.0" layoutY="107.0" text="Password:" />
      <Label layoutX="94.0" layoutY="157.0" text="Confirm Password:" />
      <TextField fx:id="usernameField" layoutX="200.0" layoutY="62.0" />
      <Button layoutX="246.0" layoutY="199.0" mnemonicParsing="false" onAction="#handleSignUp" text="Sign up" />
      <Button layoutX="23.0" layoutY="211.0" mnemonicParsing="false" onAction="#handleBackToLogin" text="Back to Login" />
      <Label fx:id="errorLabel" layoutX="217.0" layoutY="22.0" />
      <PasswordField fx:id="passwordField" layoutX="200.0" layoutY="103.0" />
      <PasswordField fx:id="confirmPasswordField" layoutX="200.0" layoutY="153.0" />
   </children>
</AnchorPane>
